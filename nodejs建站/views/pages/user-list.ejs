<% include  ../common/header.ejs %>
	<div class="container">
		<table class="table table-bordered table-hover">
      <thead>
        <tr>
          <th>用户名</th>
          <th>注册时间</th>
          <th>操作</th>
        </tr>
      </thead>
      <tbody>
        <% users.forEach(function(item){ %>
					<tr>
						<td><%= item.name %></td>
						<td><%= moment(item.meta.createAt).format('MM/DD/YYYY') %></td>
						<td><button class="btn btn-danger" id="userDelete" data-id='<%= item._id %>'>删除</button></td>
					</tr>
        <% }) %>
      </tbody>
    </table>
	</div>
<% include  ../common/footer.ejs %>